{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API server for Ultimate Optimizer App",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "migrate": "node run-migrations.js",
    "migrate:direct": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "migrate:create": "node-pg-migrate create",
    "db:test": "tsx src/db/test-connection.ts",
    "r2:test": "tsx src/utils/testR2.ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.930.0",
    "@aws-sdk/lib-storage": "^3.930.0",
    "@wix/app-management": "^1.0.122",
    "@wix/blog": "^1.0.504",
    "@wix/media": "^1.0.0",
    "@wix/pricing-plans": "^1.0.237",
    "@wix/sdk": "^1.17.1",
    "@wix/stores": "^1.0.547",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "replicate": "^1.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^4.0.5",
    "node-pg-migrate": "^6.2.2",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.5"
  }
}
